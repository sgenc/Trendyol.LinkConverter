{
  "ApplicationName": "elastic-search-app",
  "SeriLog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning" 
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://elasticsearch:9200"
  },
  "RedisCache": {
    "ConnectionString": "redis:6379"
  },
  "AllowedHosts": "*"
}
